{
    "compilerOptions": {
        "target": "ES6",
        "lib": [
            "es2015",
            "dom"
        ],
        "module": "commonjs",
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "noEmitHelpers": true,
        "noEmitOnError": true,
        "outDir": "./dist",
        "sourceMap": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "baseUrl": "./node_modules",
        "allowUnusedLabels": true,
        "rootDir": "./src",
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": [
        "src/xjs/*.ts",
        "src/pre-processors/*.ts",
    ],
    "exclude": [],
    "compileOnSave": false
}